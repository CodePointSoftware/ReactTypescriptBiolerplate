{
    "name": "typescriptreactboilerplate",
    "version": "1.0.0",
    "description": "React Typescript Boilerplate",
    "main": "index.js",
    "jest": {
        "setupFiles": [
            "<rootDir>/test-shim.js",
            "<rootDir>/test-setup.js"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js"
        },
        "testMatch": [
            "**/__tests__/*.(ts|tsx|js)"
        ]
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router": "^6.16.0",
        "react-router-dom": "^6.16.0",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^4.1.1",
        "@types/jest": "^29.5.5",
        "@types/react": "^18.2.22",
        "@types/react-dom": "^18.2.7",
        "@types/react-redux": "^7.1.24",
        "@types/react-router": "^5.1.20",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^5.33.0",
        "@typescript-eslint/parser": "^5.33.0",
        "@vitejs/plugin-react": "^4.1.0",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "prettier": "^3.0.3",
        "react-test-renderer": "^18.2.0",
        "sass": "^1.68.0",
        "ts-jest": "^28.0.7",
        "ts-loader": "^9.3.1",
        "typescript": "^5.2.2",
        "vite": "^4.4.9",
        "vite-plugin-html": "^3.2.0",
        "vite-plugin-html-purgecss": "^0.1.1"
    },
    "scripts": {
        "test": "jest --passWithNoTests",
        "start": "vite serve",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "prettier:fix": "./node_modules/.bin/prettier './src/**/*.(ts|tsx|scss)' --write",
        "postinstall": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/krzysztofgrowinski/ReactTypescriptBiolerplate.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/krzysztofgrowinski/ReactTypescriptBiolerplate/issues"
    },
    "homepage": "https://github.com/krzysztofgrowinski/ReactTypescriptBiolerplate#readme",
    "packageManager": "yarn@3.1.1"
}
