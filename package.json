{
    "name": "typescriptreactboilerplate",
    "version": "1.0.0",
    "description": "React Typescript Boilerplate",
    "main": "index.js",
    "jest": {
        "setupFiles": [
            "<rootDir>/test-shim.js",
            "<rootDir>/test-setup.js"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js"
        },
        "testMatch": [
            "**/__tests__/*.(ts|tsx|js)"
        ]
    },
    "dependencies": {
        "@types/react-router-dom": "^5.3.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router": "^6.3.0",
        "react-router-dom": "^6.3.0",
        "rxjs": "^7.5.6",
        "sass": "^1.54.4",
        "typescript": "^4.7.4"
    },
    "devDependencies": {
        "@babel/core": "^7.18.10",
        "@types/enzyme": "^3.10.12",
        "@types/jest": "^28.1.6",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "@types/react-redux": "^7.1.24",
        "@typescript-eslint/eslint-plugin": "^5.33.0",
        "@typescript-eslint/parser": "^5.33.0",
        "babel-loader": "^8.2.5",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.7.1",
        "dotenv-webpack": "^8.0.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.6",
        "enzyme-to-json": "^3.6.2",
        "eslint": "^8.21.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^7.0.4",
        "jest": "^28.1.3",
        "mini-css-extract-plugin": "^2.6.1",
        "optimize-css-assets-webpack-plugin": "^6.0.1",
        "pinst": "^3.0.0",
        "prettier": "^2.7.1",
        "react-test-renderer": "^18.2.0",
        "sass-loader": "^13.0.2",
        "source-map-loader": "^4.0.0",
        "style-loader": "^3.3.1",
        "terser-webpack-plugin": "^5.3.3",
        "ts-jest": "^28.0.7",
        "ts-loader": "^9.3.1",
        "url-loader": "^4.1.1",
        "webpack": "^5.76.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-middleware": "^5.3.3",
        "webpack-dev-server": "^4.9.3",
        "webpack-hot-middleware": "^2.25.1"
    },
    "scripts": {
        "test": "jest --passWithNoTests",
        "start": "webpack serve --config ./webpack.config.js --mode development",
        "dev:build-js": "webpack --config ./webpack.config.js --mode development",
        "prod:build-js": "webpack --config ./webpack.config.js --mode production",
        "prettier:fix": "./node_modules/.bin/prettier './src/**/*.ts' --write",
        "postinstall": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/krzysztofgrowinski/ReactTypescriptBiolerplate.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/krzysztofgrowinski/ReactTypescriptBiolerplate/issues"
    },
    "homepage": "https://github.com/krzysztofgrowinski/ReactTypescriptBiolerplate#readme",
    "packageManager": "yarn@3.1.1"
}
